<!DOCTYPE html>
<html ng-app>
    
    <head>
        <title>EEB Hub Simulation Platform Tools</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Bootstrap -->
        <link href="css/bootstrap.min.css" rel="stylesheet">
        <link href="css/stylesheet.css" rel="stylesheet">
        <link href="css/partial.css" rel="stylesheet">
        <link href="css/bootstrap-responsive.min.css" rel="stylesheet">
        <style type="text/css">
            body {
                padding-top: 60px;
                padding-bottom: 40px;
                background-color: rgb(229, 229, 229);
            }
        </style>
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.7/angular.min.js"></script>
        <script src="js/building101Data.js"></script>
        
        <script>
        function buildingInfoPop(){
            
                var form = document.forms.platformForm;
                form.building_name.value = "<%= bldname %>";
                form.activity_type.value = "<%=  bld_function %>";
                form.weather_epw_location.value = "<%= bld_location %>";
                form.year_completed.value = "<%= bld_year %>";
                form.room_width.value = "<%= room_width %>";
                form.room_depth.value = "<%= room_depth %>";
                form.room_height.value = "<%= room_height %>";
                form.exterior_shading_orientation.value = "<%= exterior_shading_orientation %>";
                form.window_to_wall_ratio.value = "<%= window_to_wall_ratio %>";
                form.number_of_floors.value = "<%= number_of_floors %>";
                form.overhang_depth.value = "<%= overhang_depth %>";
                form.wall_insulation_r_value.value = "<%= wall_insulation_r_value %>";
                form.thermal_mass.value = "<%= thermal_mass %>";
                form.window_glass_coating.value = "<%= window_glass_coating %>";
                form.window_glass_type.value = "<%= window_glass_type %>";
                form.roof_type.value = "<%= roof_type %>";
                form.roof_insulation_type.value = "<%= roof_insulation_type %>";
                form.roof_insulation_location.value= "<%= roof_insulation_location %>";
                form.people_density.value= "<%= people_density %>";
                form.illuminance.value= "<%= illuminance %>";
                form.equipment_power_density.value= "<%= equipment_power_density %>";
                form.ventilation_system.value= "<%= ventilation_system %>";
                form.weekday_occupancy_start.value = "<%= weekday_occupancy_start %>"; 
                form.weekday_occupancy_end.value = "<%= weekday_occupancy_end %>";
            
        }
        </script>
        <script>
            function saveBuildingPartial() {
                var form = document.forms.platformForm;
                form.action = "/savebuildingpartial";
                form.submit();
            }

        </script>
        
    </head>
    
    <body data-spy="scroll" data-target=".bs-docs-sidebar" onload="buildingInfoPop();">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container-fluid">
                    <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="/" class="brand">EEB Hub Simulation Platform</a>
                    <div class="nav-collapse collapse">
                        <ul class="nav">
                            <li class="">
                                <a href="/lite">Lite</a>
                            </li>
                            <li class="active">
                                <a href="/partial">Partial</a>
                            </li>
                            <li>
                                <a href="/substantial">Substantial</a>
                            </li>
                            <li>
                                <a href="/comprehensive">Comprehensive</a>
                            </li>
                        </ul>
                    </div>
                    <!--/.nav-collapse -->
                    <div class="pull-right text-right">

                        <input type="radio" name="unitoption" value="si" checked="checked" onclick='$(".unit").html("m");$("#si-ip").val("si");$("body").css("background", "rgb(229, 229, 229)");'/><font color="yellow"> SI</font>
                        <input type="radio" name="unitoption" value="ip" onclick='$(".unit").html("ft");$("#si-ip").val("ip");$("body").css("background", "rgb(194, 194, 194)");'/><font color="yellow"> IP</font>
                    
                        <ul class="nav pull-right">
						      <li><a href="/mydashboard">My Dashboard</a>
						      </li>
						      <li><a href="/logout">LOGOUT</a>
						      </li>
						</ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- Intro -->
        <div class="container">
            <div class="row">
                <div class="span3 bs-docs-sidebar">
                    <ul class="nav nav-list bs-docs-sidenav">
                        <li>
                            <h3>Partial
                                <small>-MIT</small>
                            </h3>
                        </li>
                        <li>
                            <h4 ng-cloak>{{buildingName}}</h4>
                        </li>
                        <li>
                            <a href="#building"><i class="icon-map-marker"></i> Building</a>
                        </li>
                        <li>
                            <a href="#architecture"><i class="icon-home"></i> Architecture</a>
                        </li>
                        <li>
                            <a href="#materials"><i class="icon-th-large"></i> Materials</a>
                        </li>
                        <li>
                            <a href="#people"><i class="icon-user"></i> People</a>
                        </li>
                        <li>
                            <a href="#lighting"><i class="icon-asterisk"></i> Lighting</a>
                        </li>
                        <li>
                            <a href="#mechanical"><i class="icon-cog"></i> Mechanical</a>
                        </li>
                         <li>
                            <a href="#schedules"><i class="icon-time"></i>Schedules</a>
                        </li>
                    </ul>
                </div>
                <div class="span9">
                    <form action="/partialanalysis" method="post" name="platformForm">
                        <section id="building">
                            <div class="box-unit">
                                <div class="page-header">
                                    <table>
                                        <tr>
                                            <td>
                                                <h2>Building</h2>
                                            </td>
                                            <td class="span7">
                                                <div class="pull-right text-right">
                                                
                                                <button type="button" onclick="building101Data_partial()" class="btn">
                                                    <i class="icon-plus"></i><b>Building 101 Data</b></button>
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Building Name</h4>
                                        <input class="span3" type="text" ng-model="buildingName"
                                        name="building_name" >
                                        <br/>
                                        <h4>Location</h4>
                                        <div class="input-append">
                                            <select class="span3" name="weather_epw_location">
                                                <option>Albuquerque, NM</option>
                                                <option>Baltimore, MD</option>
                                                <option>Boise, ID</option>
                                                <option>Burlington, VT</option>
                                                <option>Chicago, IL</option>
                                                <option>Duluth, MN</option>
                                                <option>El Paso, TX</option>
                                                <option>Fairbanks, AK</option>
                                                <option>Helena, MT</option>
                                                <option>Houston, TX</option>
                                                <option>Memphis, TN</option>
                                                <option>Miami, FL</option>
                                                <option>Pheonix, AZ</option>
                                                <option selected>Philadelphia, PA</option>
                                                <option>Riyadh, SAU</option>
                                                <option>Salem, OR</option>
                                                <option>San Francisco, CA</option>
                                                <option>Vancouver, CAN</option>
                                            </select>
                                            <span class="add-on">
                                                <a href="http://developer.eebhub.org/weather.html" target="_blank"><i class="icon-map-marker"></i></a>
                                            </span>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <h4>Function</h4>
                                         <div class="input-append">
                                            <select class="span3" name="activity_type">
                                                <option>Small Office</option>
                                                <option>Medium Office</option>
                                                <option>Large Office</option>
                                                <option>Stand-alone Retail</option>
                                                <option>Strip Mall</option>
                                                <option>Primary School</option>
                                                <option>Secondary School</option>
                                                <option>Outpatient Healthcare</option>
                                                <option>Hospital</option>
                                                <option>Small hotel</option>
                                                <option>Large Hotel</option>
                                                <option>Warehouse (non-refrigerated)</option>
                                                <option>Quick Service Restaurant</option>
                                                <option>Full Service Restaurant</option>
                                                <option>Mid-Rise Apartment</option>
                                                <option>High-Rise Apartment</option>
                                            </select>
                                            <span class="add-on">
                                                <a href="http://www1.eere.energy.gov/buildings/commercial/ref_buildings.html" target="_blank"><i class="icon-info-sign"></i></a>
                                            </span>
                                        </div>
                                        <h4>Year Built</h4>
                                        <input class="span3" type="number" min="1500" max="2020" step="1" id="year_completed" name="year_completed">
                                    </div>
                                </div>
                        </section>
                        <section id="architecture">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Architecture
                                        <small>typical room</small>
                                    </h2>
                                </div>
                                <div class="row">
                                    <div class="span2">
                                        <img class="img-rounded" src="img/room_dimensions.png">
                                    </div>
                                    <div class="span3" id="architect-replace1">
                                        <h4>Room Width</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.001" class="span2" id="appendedInput"
                                            name="room_width" />
                                            <span class="add-on"><span class="unit">m</span></span>
                                        </div>
                                        <h4>Room Depth</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.001" class="span2" id="appendedInput"
                                            name="room_depth" />
                                            <span class="add-on"><span class="unit">m</span></span>
                                        </div>
                                        <h4>Room Height</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.001" class="span2" id="appendedInput"
                                            name="room_height" />
                                            <span class="add-on"><span class="unit">m</span></span>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <h4>Window Facade Orientation</h4>
                                        <select name="exterior_shading_orientation"
                                        class="span2">
                                            <option>North</option>
                                            <option>North-East</option>
                                            <option>East</option>
                                            <option>South-East</option>
                                            <option>South</option>
                                            <option>South-West</option>
                                            <option>West</option>
                                            <option>North-West</option>
                                        </select>
                                        <h4>% Window to Wall Area</h4>
                                        <ul class="inline">
                                            <li>
                                                <input type="range" ng-model="winwall" min="10" max="60" step="1" value=50 />
                                            </li>
                                            <li>
                                                <div class="input-append">
                                                    <input class="span1" id="window_to_wall_ratio" type="text" value={{winwall}} ng-model="winwall" name="window_to_wall_ratio">
                                                    <span class="add-on">%</span>
                                                </div>
                                            </li>
                                        </ul>
                                        <h4>Number of Floors</h4>
                                        <input type="number" id="number_of_floors" name="number_of_floors" min="0" step="1">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="span2">
                                        <img class="img-rounded" src="img/overhang.gif" width="170">
                                    </div>
                                    <div class="span3" id="architect-replace2">
                                        <h4>Overhang Depth</h4>
                                        <div class="input-append">
                                            <input class="span2" id="appendedInput" type="number" name="overhang_depth">
                                            <span class="add-on"><span class="unit">m</span></span>
                                        </div>
                                    </div>
                                </div>
                                <h2 class="text-right">
                                    <small>Single Zone (1 facade) defaulted</small>
                                </h2>
                            </div>
                        </section>
                        <section id="materials">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Materials</h2>
                                </div>
                                <div class="row ">
                                    <div class="span4 ">
                                        <h4>Wall Insulation</h4>
                                        <select class="span3 " name="wall_insulation_r_value">
                                            <option value="1">Commercial (Low Insulation): 1(m2-&deg;C)/W</option>
                                            <option value="2">Commercial (Medium Insulation): 2(m2-&deg;C)/W</option>
                                            <option value="5">Commercial (High Insulation): 5(m2-&deg;C)/W</option>
                                        </select>
                                        <h4>Floor Thermal Mass</h4>
                                        <select class="span3 " name="thermal_mass">
                                            <option value="high">High: think concrete slab or iron girder construction</option>
                                            <option
                                            value="low">Low: lightweight or wood framed with no concrete</option>
                                                <option value="zero">Zero</option>
                                        </select>
                                        <h4>Windows</h4>
                                        <select class="span3 " name="window_glass_type">
                                            <option value="sgu_nb">Single Glazed</option>
                                            <option value="dgu_nb">Double Glazed</option>
                                            <option value="tgu_nb">Triple Glazed</option>
                                        </select>
                                        <h4>Glass Coating</h4>
                                        <select name="window_glass_coating">
                                            <option>clear</option>
                                            <option>low-e</option>
                                            <option>High Performance</option>
                                            <option>Blue</option>
                                            <option>Bronze</option>
                                            <option>Green</option>
                                            <option>Grey</option>
                                        </select>
                                    </div>
                                    <div class="span4 ">
                                        <h4>Roof</h4>
                                        <select class="span3 " name="roof_type">
                                            <option value="none">Do Not Consider</option>
                                            <option value="bitumen">Modified Bitumen Roof</option>
                                            <option value="green">Green Roof</option>
                                            <option value="cool">Cool Roof</option>
                                        </select>
                                        <h4>Roof Insulation</h4>
                                        <select class="span3 " name="roof_insulation_type">
                                            <option value="1">Commercial (Low Insulation): 1(m2-&deg;C)/W</option>
                                            <option value="2">Commercial (Medium Insulation): 2(m2-&deg;C)/W</option>
                                            <option value="5">Commercial (High Insulation): 5(m2-&deg;C)/W</option>
                                        </select>
                                        <h4>Roof Insulation Location</h4>
                                        <select class="span3" name="roof_insulation_location">
                                            <option value="top">Top of Roof Slab</option>
                                            <option value="bottom">Bottom of Roof Slab</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="people">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>People</h2>
                                </div>
                                <h4>People Density</h4>
                                <div class="input-append">
                                    <select class="span2" id="appendedInput" name="people_density">
                                        <option>0.000</option>
                                        <option>0.025</option>
                                        <option>0.050</option>
                                        <option>0.075</option>
                                        <option>0.10</option>
                                        <option>0.25</option>
                                        <option>0.50</option>
                                        <option>0.75</option>
                                        <option>1.0</option>
                                        <option>1.5</option>
                                        <option>2.0</option>
                                    </select>
                                    <span class="add-on" id="people-replace">People/m&sup2;</span>
                                </div>
                            </div>
                        </section>
                        <section id="lighting">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Lighting</h2>
                                </div>
                                <h4>Lighting Intensity</h4>
                                <div class="input-append">
                                    <select class="span2" id="appendedInput" name="illuminance">
                                        <option value="0">0 (Darkness)</option>
                                        <option value="150">150 (Storage)</option>
                                        <option value="200">200 (Casual Work)</option>
                                        <option value="300">300 (Rough Work)</option>
                                        <option value="400">400 Office Work-EU)</option>
                                        <option value="500">500 (Office Work-US)</option>
                                        <option value="750">750 (Fine Work)</option>
                                        <option value="1000">1000 (Fine Assembly)</option>
                                        <option value="1500">1500 (Very Fine Work)</option>
                                    </select>
                                    <span class="add-on">lux</span>
                                </div>
                            </div>
                        </section>
                        <section id="mechanical">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Mechanical</h2>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Equipment Density</h4>
                                        <div class="input-append">
                                            <select class="span2" id="appendedInput" name="equipment_power_density">
                                                <option value="0.00">0.0 No Equipment</option>
                                                <option value="1.00">1.0</option>
                                                <option value="5.00">5.0 Office (light)</option>
                                                <option value="10.00">10 Office (medium)</option>
                                                <option value="15.00">15 Office (medium-high)</option>
                                                <option value="20.00">20 Office (high)</option>
                                                <option value="25.00">25</option>
                                                <option value="50.00">50</option>
                                                <option value="100.00">100</option>
                                            </select>
                                            <span class="add-on" id="mechanical-replace">W/m<sup>2</sup></span>
                                        </div>
                                    </div>
                                    <div class="span4">
                                        <h4>Ventilation System</h4>
                                        <select name="ventilation_system">
                                            <option value="mechanical">Mechanical Cooling & Heating</option>
                                            <option value="natural">Natural ventilation cooling, Mechanical Heating</option>
                                            <option value="joint">Joint natural ventilation cooling and mechanical heating</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="schedules">
                            <div class="box-unit ">
                                <div class="page-header ">
                                    <h2>Schedules</h2>
                                </div>
                                <div class="row ">
                                    <div class="span4 ">
                                        <h4>Occupancy Start</h4>
                                        <select name="weekday_occupancy_start">
                                            <option value="0.0">12:00 midnight</option>
                                            <option value="1.0">&nbsp;1:00 AM</option>
                                            <option value="2.0">&nbsp;2:00 AM</option>
                                            <option value="3.0">&nbsp;3:00 AM</option>
                                            <option value="4.0">&nbsp;4:00 AM</option>
                                            <option value="5.0">&nbsp;5:00 AM</option>
                                            <option value="6.0">&nbsp;6:00 AM</option>
                                            <option value="7.0">&nbsp;7:00 AM</option>
                                            <option value="8.0">&nbsp;8:00 AM</option>
                                            <option value="9.0">&nbsp;9:00 AM</option>
                                            <option value="10.0">10:00 AM</option>
                                            <option value="11.0">11:00 AM</option>
                                            <option value="12.0">12:00 noon</option>
                                            <option value="13.0">&nbsp;1:00 PM</option>
                                            <option value="14.0">&nbsp;2:00 PM</option>
                                            <option value="15.0">&nbsp;3:00 PM</option>
                                            <option value="16.0">&nbsp;4:00 PM</option>
                                            <option value="17.0">&nbsp;5:00 PM</option>
                                            <option value="18.0">&nbsp;6:00 PM</option>
                                            <option value="19.0">&nbsp;7:00 PM</option>
                                            <option value="20.0">&nbsp;8:00 PM</option>
                                            <option value="21.0">&nbsp;9:00 PM</option>
                                            <option value="22.0">10:00 PM</option>
                                            <option value="23.0">11:00 PM</option>
                                            <option value="24.0">12:00 midnight</option>
                                        </select>
                                    </div>
                                    <div class="span4 ">
                                        <h4>Occupancy End</h4>
                                        <select name="weekday_occupancy_end">
                                            <option value="0.0">12:00 midnight</option>
                                            <option value="1.0">&nbsp;1:00 AM</option>
                                            <option value="2.0">&nbsp;2:00 AM</option>
                                            <option value="3.0">&nbsp;3:00 AM</option>
                                            <option value="4.0">&nbsp;4:00 AM</option>
                                            <option value="5.0">&nbsp;5:00 AM</option>
                                            <option value="6.0">&nbsp;6:00 AM</option>
                                            <option value="7.0">&nbsp;7:00 AM</option>
                                            <option value="8.0">&nbsp;8:00 AM</option>
                                            <option value="9.0">&nbsp;9:00 AM</option>
                                            <option value="10.0">10:00 AM</option>
                                            <option value="11.0">11:00 AM</option>
                                            <option value="12.0">12:00 noon</option>
                                            <option value="13.0">&nbsp;1:00 PM</option>
                                            <option value="14.0">&nbsp;2:00 PM</option>
                                            <option value="15.0">&nbsp;3:00 PM</option>
                                            <option value="16.0">&nbsp;4:00 PM</option>
                                            <option value="17.0">&nbsp;5:00 PM</option>
                                            <option value="18.0">&nbsp;6:00 PM</option>
                                            <option value="19.0">&nbsp;7:00 PM</option>
                                            <option value="20.0">&nbsp;8:00 PM</option>
                                            <option value="21.0">&nbsp;9:00 PM</option>
                                            <option value="22.0">10:00 PM</option>
                                            <option value="23.0">11:00 PM</option>
                                            <option value="24.0">12:00 midnight</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <div class="box-unit">
                            <button type="submit" onclick="saveBuildingPartial();" class="btn btn-large btn-success">Save Building (partial)</button>
                            <input type="submit" class="btn btn-large btn-primary" value="SIMULATE"/>
                            <a class="btn btn-large btn-info" href="http://128.118.67.227/da_example_outputs.html"><i class="icon-signal"></i> Example Outputs</a>
                            <img style="text-align:right;float:right;" src="img/mit.png" alt="MIT" width="80" height="41">
                            <h2 class="text-right">
                                <small>powered by MIT Design Advisor Engine</small>
                            </h2>
                        </div>
                        <div class="box-unit">
                                <a class="btn btn-large btn-warning" href="http://developer.eebhub.org/utrc/uncertainty/riskanalysis.html"><i class="icon-exclamation-sign"></i> Risk Analysis </a>
                                <img style="text-align:right;float:right;" src="img/utrc.png" alt="UTRC" width="120" height="42">
                                <br>
                                <h2 class="text-right"><small>powered by UTRC Uncertainty Quantification</small></h2>
                        </div>
                        
                        <div style="visibility:hidden"><input name="unit" value = "si" id="si-ip" /></div>
                    </form>
                    </div>
                </div>
            </div>
            <!-- Javascript -->
            <script src="js/ui-bootstrap.js"></script>
            <script src="js/jquery.js"></script>
            <script src="js/bootstrap.min.js"></script>
            <script src="js/bootstrap-scrollspy.js"></script>
