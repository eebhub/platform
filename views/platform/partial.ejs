<!DOCTYPE html>
<html ng-app="partial">
    
    <head>
        <title>EEB Hub Simulation Platform Tools</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Bootstrap -->
        <link href="../css/bootstrap.min.css" rel="stylesheet">
        <link href="../css/stylesheet.css" rel="stylesheet">
        <link href="../css/partial.css" rel="stylesheet">
        <link href="../css/bootstrap-responsive.min.css" rel="stylesheet">
        <style type="text/css">
            body {
                padding-top: 60px;
                padding-bottom: 40px;
                background-color: rgb(229, 229, 229);
            }
        </style>
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.7/angular.min.js"></script>
    </head>
    
    <body data-spy="scroll" data-target=".bs-docs-sidebar">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container-fluid">
                    <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="/platform/" class="brand">EEB Hub Simulation Platform</a>
                    <div class="nav-collapse collapse">
                        <ul class="nav">
                            <li class="">
                                <a href="/platform/lite">Lite</a>
                            </li>
                            <li class="active">
                                <a href="/platform/partial">Partial</a>
                            </li>
                            <li>
                                <a href="/platform/substantial">Substantial</a>
                            </li>
                            <li>
                                <a href="/platform/comprehensive">Comprehensive</a>
                            </li>
                        </ul>
                    </div>
                    <!--/.nav-collapse -->
                </div>
            </div>
        </div>
        <!-- Intro -->
        <div class="container">
            <div class="row">
                <div class="span3 bs-docs-sidebar">
                    <ul class="nav nav-list bs-docs-sidenav">
                        <li>
                            <h3>Partial
                                <small>-MIT</small>
                            </h3>
                        </li>
                        <li>
                            <h4 ng-cloak>{{buildingName}}</h4>
                        </li>
                        <li>
                            <a href="#building"><i class="icon-chevron-right"></i> Building</a>
                        </li>
                        <li>
                            <a href="#shape"><i class="icon-chevron-right"></i> Shape/Size</a>
                        </li>
                        <li>
                            <a href="#occupancy"><i class="icon-chevron-right"></i> Occupancy</a>
                        </li>
                        <li>
                            <a href="#materials"><i class="icon-chevron-right"></i> Materials</a>
                        </li>
                        <li>
                            <a href="#mechanical"><i class="icon-chevron-right"></i> Mechanical</a>
                        </li>
                        <li>
                            <a href="#room"><i class="icon-chevron-right"></i> Typical Room</a>
                        </li>
                    </ul>
                </div>
                <div class="span9">
                    <form action="/partialanalysis" method="post">
                        <section id="building">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Building</h2>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Building Name</h4>
                                        <input class="span3" type="text" ng-model="buildingName"
                                        name="building_name">
                                        <br/>
                                        <h4>Year Built</h4>
                                        <select class="span2" name="year_completed">
                                            <option>2013</option>
                                            <option>2012</option>
                                            <option>2011</option>
                                            <option>2010</option>
                                            <option>2009</option>
                                            <option>2008</option>
                                            <option>2007</option>
                                            <option>2006</option>
                                            <option>2005</option>
                                            <option>2004</option>
                                            <option>2003</option>
                                            <option>2002</option>
                                            <option>2001</option>
                                            <option>2000</option>
                                            <option>1999</option>
                                            <option>1998</option>
                                            <option>1997</option>
                                            <option>1996</option>
                                            <option>1995</option>
                                            <option>1994</option>
                                            <option>1993</option>
                                            <option>1992</option>
                                            <option>1991</option>
                                            <option>1990</option>
                                            <option>1989</option>
                                            <option>1988</option>
                                            <option>1987</option>
                                            <option>1986</option>
                                            <option>1985</option>
                                            <option>1984</option>
                                            <option>1983</option>
                                            <option>1982</option>
                                            <option>1981</option>
                                            <option>1980</option>
                                            <option>1979</option>
                                            <option>1978</option>
                                            <option>1977</option>
                                            <option>1976</option>
                                            <option>1974</option>
                                            <option>1973</option>
                                            <option>1972</option>
                                            <option>1971</option>
                                            <option>1970</option>
                                        </select>
                                    </div>
                                    <div class="span4">
                                        <div>
                                            <h4>Function</h4>
                                            <select class="span3" name="activity_type">
                                                <option>Small Office</option>
                                                <option>Medium Office</option>
                                                <option>Large Office</option>
                                                <option>Stand-alone Retail</option>
                                                <option>Strip Mall</option>
                                                <option>Primary School</option>
                                                <option>Secondary School</option>
                                                <option>Outpatient Healthcare</option>
                                                <option>Hospital</option>
                                                <option>Small hotel</option>
                                                <option>Large Hotel</option>
                                                <option>Warehouse (non-refrigerated)</option>
                                                <option>Quick Service Restaurant</option>
                                                <option>Full Service Restaurant</option>
                                                <option>Mid-Rise Apartment</option>
                                                <option>High-Rise Apartment</option>
                                            </select>
                                        </div>
                                        <h4>City</h4>
                                        <select class="span3" name="weather_epw_location">
                                            <option>Albuquerque, NM (4B)</option>
                                            <option>Baltimore, MD (4A)</option>
                                            <option>Boise, ID (5B)</option>
                                            <option>Burlingotn, NH (6A)</option>
                                            <option>Chicago, IL (5A)</option>
                                            <option>Duluth, MN (7)</option>
                                            <option>El Paso, TX (3B)</option>
                                            <option>Fairbanks, AK (8)</option>
                                            <option>Helena, MT (6B)</option>
                                            <option>Houston, TX (2A)</option>
                                            <option>Memphis, TN (3A)</option>
                                            <option>Miami, FL (1A)</option>
                                            <option>Pheonix, AZ (2B)</option>
                                            <option>Philadelphia, PA (4A)</option>
                                            <option>Riyadh, SAU (1B)</option>
                                            <option>Salem, MA (4C)</option>
                                            <option>San Francisco, CA (3C)</option>
                                            <option>Vancouver, CAN (5C)</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="shape">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Shape/Size</h2>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Gross Floor Area</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.1" name="gross_floor_area" />
                                            <span class="add-on">m&#178;</span>
                                        </div>
                                        <h4>Number of Floors</h4>
                                        <input type="number" name="number_of_floors" min="0"
                                        step="1" />
                                    </div>
                                    <div class="span4">
                                        <h4>Length</h4>
                                        <div class="input-append">
                                            <input type="number" name="building_length" min="0" step="0.01" />
                                            <span class="add-on">m</span>
                                        </div>
                                        <h4>Width</h4>
                                        <div class="input-append">
                                            <input type="number" name="building_width" min="0" step="0.01" />
                                            <span class="add-on">m</span>
                                        </div>
                                        <h4>Window/Wall Ratio</h4>
                                        <ul class="inline">
                                            <li>
                                                <input type="range" ng-model="winwall" name="window_to_wall_ratio" min="0"
                                                max="40" step="1" value=50/>
                                            </li>
                                            <li>
                                                <div class="input-append">
                                                    <input class="span1" type="text" value={{winwall}} ng-model="winwall"
                                                    />
                                                    <span class="add-on">%</span>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="span3">
                                        <h4>Shape</h4>
                                        <select name="" ng-model="shapeimage" name="footprint_shape">
                                            <option value="../img/basic_geometry/c_shape_min_zone.png">C Shape</option>
                                            <option value="../img/basic_geometry/h_shape_min_zone.png">H Shape</option>
                                            <option value="../img/basic_geometry/l_shape_min_zone.png">L Shape</option>
                                            <option value="../img/basic_geometry/rect_min_zone.png">Rectangle</option>
                                            <option value="../img/basic_geometry/t_shape_min_zone.png">T Shaped</option>
                                            <option value="../img/basic_geometry/u_shape_min_zone.png">U Shaped</option>
                                        </select>
                                        <h4>Orientation</h4>
                                        <table>
                                            <tr>
                                                <td></td>
                                                <td></td>
                                                <td>
                                                    <button class="btn" type="button" ng-click="orientation='north'">North</button>
                                                </td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td></td>
                                                <td>
                                                    <button class="btn" type="button" ng-click="orientation='northwest'">NW</button>
                                                </td>
                                                <td></td>
                                                <td>
                                                    <button class="btn" type="button" ng-click="orientation='northeast'">NE</button>
                                                </td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    <button class="btn" type="button" ng-click="orientation='west'">West</button>
                                                </td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td>
                                                    <button class="btn" type="button" ng-click="orientation='east'">East</button>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td></td>
                                                <td>
                                                    <button class="btn" type="button " ng-click="orientation='southwest' ">SW</button>
                                                </td>
                                                <td></td>
                                                <td>
                                                    <button class="btn
                                                " type="button " ng-click="orientation='southeast' ">SE</button>
                                                </td>
                                                <td></td>
                                            </tr>
                                            <tr>
                                                <td></td>
                                                <td></td>
                                                <td>
                                                    <button class="btn
                                                " type="button " ng-click="orientation='south' ">South</button>
                                                </td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                        </table>
                                    </div>
                                    <div class="span5 ">
                                        <img ng-src="{{shapeimage}}" ng-class="orientation" ng-name="orientation"
                                        />
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="occupancy">
                            <div class="box-unit ">
                                <div class="page-header ">
                                    <h2>Occupancy</h2>
                                </div>
                                <div class="row ">
                                    <div class="span4 ">
                                        <h4>Weekday Occupancy Hours</h4>
                                        <h5>Day Start</h5>
                                        <input type="time" ng-model="weekdayStart
                                                " name="weekday_occupancy_start"
                                        />
                                        <h5>Day End</h5>
                                        <input type="time" value={{weekdayStart}} min={{weekdayStart}}
                                        name="weekday_occupancy_end" />
                                    </div>
                                    <div class="span4 ">
                                        <h4>Weekend Occupancy Hours</h4>
                                        <h5>Day Start</h5>
                                        <input type="time" ng-model="weekendStart" name="weekend_occupancy_start"
                                        />
                                        <h5>Day End</h5>
                                        <input type="time" value={{weekendStart}} min={{weekendStart}}
                                        name="weekend_occupancy_end" />
                                        <h4>Number of Occupants</h4>
                                        <input type="number" step="1" min="0 " name="number_of_occupants"
                                        />
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="materials">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Materials</h2>
                                </div>
                                <div class="row ">
                                    <div class="span4 ">
                                        <h4>Walls</h4>
                                        <select class="span3 " name="exterior_wall_type">
                                            <option>Concrete Mass</option>
                                            <option>Metal</option>
                                            <option>Steel Framed</option>
                                            <option>Wood Framed and Other</option>
                                        </select>
                                        <h4>Windows</h4>
                                        <select class="span3 " name="window_glass_type">
                                            <option>Multi-layer Glass</option>
                                            <option>Single Layer Glass</option>
                                            <option>Combination</option>
                                            <option>No Windows</option>
                                            <option>Unknown</option>
                                        </select>
                                        <h4>Thermal Mass</h4>
                                        <select class="span3 " name="thermal_mass">
                                            <option>High</option>
                                            <option>Low</option>
                                            <option>None</option>
                                            <option>Isolated in Floor</option>
                                        </select>
                                    </div>
                                    <div class="span4 ">
                                        <h4>Roof</h4>
                                        <select class="span3 " name="roof_type">
                                            <option>Modified Bitumen Roof</option>
                                            <option>Metal Roof</option>
                                            <option>Green Roof</option>
                                            <option>Cool Roof</option>
                                        </select>
                                        <h4>Roof Insulation Type</h4>
                                        <select class="span3 " name="roof_insulation_type">
                                            <option>Wood Fiber</option>
                                            <option>Perlite Lnsulation Board</option>
                                            <option>Polyisocyanurate</option>
                                            <option>Polystyrene</option>
                                            <option>Extruded Polystyrene</option>
                                            <option>Cellular Glass</option>
                                            <option>Gypsum Board</option>
                                        </select>
                                        <h4>Wall Insulation R Value</h4>
                                        <div class="input-append ">
                                            <input class="span3 " id="appendedInput" type="text" name="wall_insulation_r_value"
                                            />
                                            <span class="add-on ">m
                                                <sup>2</sup>C/W</span>
                                        </div>
                                        <h4>Roof Insulation Location</h4>
                                        <select class="span3" name="roof_insulation_location">
                                            <option>Top of Slab</option>
                                            <option>Bottom of Slab</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="mechanical">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Mechanical</h2>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Ventilation System</h4>
                                        <select name="ventilation_system">
                                            <option>Mechanical Cooling & Heating</option>
                                            <option>Natural ventilation cooling, Mechanical Heating</option>
                                            <option>Joint natural ventilation cooling and mechanical heating</option>
                                        </select>
                                    </div>
                                    <div class="span4">
                                        <h4>Equipment Density</h4>
                                        <div class="input-append">
                                            <select class="span2" id="appendedInput" name="equipment_power_density">
                                                <option>0.00 No Equipment</option>
                                                <option>1.00</option>
                                                <option>1.00 Office (Small)</option>
                                                <option>5.00 Office (Medium)</option>
                                                <option>10.00 Office (Large)</option>
                                                <option>15</option>
                                                <option>20</option>
                                                <option>25</option>
                                                <option>50</option>
                                                <option>100</option>
                                            </select>
                                            <span class="add-on">W/m
                                                <sup>2</sup>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section id="room">
                            <div class="box-unit">
                                <div class="page-header">
                                    <h2>Typical Room</h2>
                                </div>
                                <div class="row">
                                    <div class="span3">
                                        <h4>Room Width</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.1" class="span2" id="appendedInput"
                                            name="room_width" />
                                            <span class="add-on">m</span>
                                        </div>
                                        <h4>Room Depth</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.1" class="span2" id="appendedInput"
                                            name="room_depth" />
                                            <span class="add-on">m</span>
                                        </div>
                                        <h4>Room Height</h4>
                                        <div class="input-append">
                                            <input type="number" min="0" step="0.1" class="span2" id="appendedInput"
                                            name="room_height" />
                                            <span class="add-on">m</span>
                                        </div>
                                    </div>
                                    <div class="span3">
                                        <h4>Window/Primary Facade Orientation</h4>
                                        <select name="exterior_shading_orientation"
                                        class="span2">
                                            <option>North</option>
                                            <option>North-East</option>
                                            <option>East</option>
                                            <option>South-East</option>
                                            <option>South</option>
                                            <option>South-West</option>
                                            <option>West</option>
                                            <option>North-West</option>
                                        </select>
                                        <h4>Window Area</h4>
                                        <ul class="inline">
                                            <li>
                                                <input type="range" ng-model="winwalltyp" name="window_to_wall_ratio_room"
                                                min="0" max="40" step="1" value=50/>
                                            </li>
                                            <li>
                                                <div class="input-append">
                                                    <input class="span1" type="text" value={{winwalltyp}} ng-model="winwalltyp"
                                                    />
                                                    <span class="add-on">%</span>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="span2">
                                        <img class="../img-rounded" src="../img/room_dimensions.png">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="span2">
                                        <img class="../img-polaroid" src="../img/windows/dgu_nb_thumb_on.gif">
                                        <img class="../img-polaroid" src="../img/windows/dgu_thumb_on.gif">
                                    </div>
                                    <div class="span2">
                                        <img class="../img-polaroid" src="../img/windows/invent_thumb_on.gif">
                                        <img class="../img-polaroid" src="../img/windows/outvent_thumb_on.gif">
                                    </div>
                                    <div class="span2">
                                        <img class="../img-polaroid" src="../img/windows/sgu_nb_thumb_on.gif">
                                        <img class="../img-polaroid" src="../img/windows/sgu_thumb_on.gif">
                                    </div>
                                    <div class="span2">
                                        <img class="../img-polaroid" src="../img/windows/tgu_nb_thumb_on.gif">
                                        <img class="../img-polaroid" src="../img/windows/tgu_thumb_on.gif">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="span4">
                                        <h4>Glas Type</h4>
                                        <select name="window_glass_coating">
                                            <option>clear</option>
                                            <option>low-e</option>
                                            <option>High Performance</option>
                                            <option>Blue</option>
                                            <option>Bronze</option>
                                            <option>Green</option>
                                            <option>Grey</option>
                                        </select>
                                    </div>
                                    <div class="span4">
                                        <h4>Overhang Distance</h4>
                                        <div class="input-append">
                                            <input class="span2" id="appendedInput" type="number" name="overhang_depth">
                                            <span class="add-on">m</span>
                                        </div>
                                        <h4>Lighting</h4>
                                        <div class="input-append">
                                            <select class="span2" id="appendedInput" name="illuminance">
                                                <option>0 (Darkness)</option>
                                                <option>150 (Storage)</option>
                                                <option>200 (Casual Work)</option>
                                                <option>300 (Rough Work)</option>
                                                <option>400 Office Work-EU)</option>
                                                <option>500 (Office Work-US)</option>
                                                <option>750 (Fine Work)</option>
                                                <option>1000 (Fine Assembly)</option>
                                                <option>1500 (Very Fine Work)</option>
                                            </select>
                                            <span class="add-on">lux</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <div class="box-unit">
                            <input type="submit" class="btn-large btn-success" value="Run Design Advisor"
                            />
                            <h3>
                                <small>by MIT</small>
                            </h3>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <!-- Javascript -->
        <script src="../js/ui-bootstrap.js"></script>
        <script src="../js/jquery.js"></script>
        <script src="../js/bootstrap.min.js"></script>
        <script src="../js/bootstrap-scrollspy.js"></script>
    </body>

</html>