            }
            
            function buildingInfoPop(){
            
                    var form = document.forms.platformForm;
                    form.building_name.value = "<%= bldname %>";
                    form.activity_type.value = "<%=  bld_function %>";
                    form.weather_epw_location.value = "<%= bld_location %>";
                    form.year_completed.value = "<%= bld_year %>";
                    var gross_floor_area = "<%= gross_floor_area %>";
                    form.gross_floor_area.value = parseFloat(gross_floor_area);
                
            }
